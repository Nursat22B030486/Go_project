# This example only for local development purpose
APP_PORT=8888 # application port
APP_ENV=development # development|production|test
APP_FILL=true # true|false (fill data on app start)
APP_MIGRATIONS=file://migrations # path to migrations files
APP_DSN=postgresql://postgres:pa55word@db:5432/readit?sslmode=disable # database connection string, for example "postgresql://postgres:pa55word@localhost:5432/readit?sslmode=disable", have to be provided on CLI

# DB config
POSTGRES_USER=postgres # database user
POSTGRES_DB=readit # database name
POSTGRES_PASSWORD=pa55word # database password